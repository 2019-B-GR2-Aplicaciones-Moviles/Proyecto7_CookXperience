<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Perfil" android:background="#9C9359">

    <TextView
            android:text="Text"
            android:layout_width="238dp"
            android:layout_height="40dp" android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginTop="52dp"
            android:textColor="@android:color/background_dark" android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintHorizontal_bias="0.496"/>
    <TextView
            android:layout_width="110dp"
            android:layout_height="50dp" android:id="@+id/textView8"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="44dp"
            android:textColor="@android:color/background_dark" android:textSize="44sp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintHorizontal_bias="0.498" android:text="@string/textViewPerfil"/>
    <ImageView
            android:layout_width="154dp"
            android:layout_height="110dp" app:srcCompat="@android:drawable/sym_def_app_icon"
            android:id="@+id/imageView2" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintHorizontal_bias="0.497"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText10"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:background="@android:color/background_light"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="36dp"
            android:hint="@string/editTextFechaNacimiento"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText11" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/editText10" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:background="@android:color/background_light"
            app:layout_constraintHorizontal_bias="0.0" android:hint="@string/editTextComidaFavorita"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText12" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/editText11" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:background="@android:color/background_light"
            app:layout_constraintHorizontal_bias="0.0" android:hint="@string/edittextProfeión"/>
    <TextView
            android:text="Puntuación"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/editText12" app:layout_constraintEnd_toStartOf="@+id/textView10"
            android:layout_marginEnd="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.171"/>
    <TextView
            android:text="25"
            android:layout_width="28dp"
            android:layout_height="26dp"
            android:id="@+id/textView10" android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="44dp" app:layout_constraintTop_toBottomOf="@+id/editText12"
            app:layout_constraintStart_toEndOf="@+id/textView9" android:layout_marginStart="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>